{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/CSS/(.*)",
      "dest": "/CSS/$1"
    },
    {
      "src": "/JS/(.*)",
      "dest": "/JS/$1"
    },
    {
      "src": "/images.1/(.*)",
      "dest": "/images.1/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/HTML/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
