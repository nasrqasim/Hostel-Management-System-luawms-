{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "HTML/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "CSS/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "JS/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "images.1/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/HTML/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
